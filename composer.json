{
    "name": "timephp/console",
    "description": "Laravel-Zero CLI to TimePHP framework",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli", "TimePHP"],
    "homepage": "https://github.com/TimePHP-org/TimePHP-Console",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "support": {
        "email": "timephp.org@gmail.com",
        "docs": "https://timephp-org.github.io/TimePHP-Docs/#/",
        "issues": "https://github.com/TimePHP-org/TimePHP-Console/issues",
        "source": "https://github.com/TimePHP-org/TimePHP-Console"
    },
    "authors": [
        {
            "name": "Robin B",
            "email": "timephp.org@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "laravel-zero/framework": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["cli"]
}
